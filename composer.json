{
    "name": "imemento/activity-log",
    "description": "Activity Log for Laravel",
    "homepage": "https://github.com/mementohub/activity-log",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "guzzlehttp/guzzle": "^6.3 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0",
        "orchestra/testbench": "^3.8 || ^4.0 || ^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\ActivityLog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\ActivityLog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
